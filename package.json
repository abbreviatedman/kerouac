{
	"name": "kerouac",
	"displayName": "Kerouac",
	"description": "A Visual Studio Code extension to encourage separation of writing and editing modes.",
	"version": "0.1.0",
	"engines": {
		"vscode": "^1.29.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.kerouac.writingMode",
				"title": "Kerouac - Writing Mode"
			},
			{
				"command": "extension.kerouac.editingMode",
				"title": "Kerouac - Editing Mode"
			}
		]
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"vscode": "^1.1.28",
		"eslint": "^5.13.0"
	}
}
