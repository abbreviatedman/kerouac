{
	"name": "kerouac",
	"displayName": "Kerouac",
	"description": "A Visual Studio Code extension to encourage separation of writing and editing modes.",
	"publisher": "colin-jaffe",
	"version": "0.1.0",
	"engines": {
		"vscode": "^1.29.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:kerouac.writingMode",
		"onCommand:kerouac.editingMode",
		"onCommand:kerouac.toggleMode"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "kerouac.writingMode",
				"title": "Kerouac - Writing Mode"
			},
			{
				"command": "kerouac.editingMode",
				"title": "Kerouac - Editing Mode"
			},
			{
				"command": "kerouac.toggleMode",
				"title": "Kerouac - Toggle Writing/Editing Mode"
			}
		]
	},
	"devDependencies": {
		"vscode": "^1.1.28",
		"eslint": "^5.13.0"
	}
}
